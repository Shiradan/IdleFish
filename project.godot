config_version=5

[application]

config/name="IdleFish"
run/main_scene="res://login_page.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"

[audio]

driver/mix_rate.web=44100

[autoload]

Nakama="*res://addons/com.heroiclabs.nakama/Nakama.gd"
ServerConnection="*res://autoload/ServerConnection.gd"

[debug]

multirun/number_of_windows=2
multirun/window_distance=1270
multirun/add_custom_args=true
multirun/first_window_args="listen"
multirun/other_window_args="join"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/multirun/plugin.cfg")

[gui]

theme/custom_font="res://assets/fonts/YouSheYuFeiTeJianKangTi-2.ttf"

